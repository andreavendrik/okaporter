.page-content
  max-width 720px
  background-color rgba(255,255,255,0.95)
  margin auto
  z-index 30
  padding 20px
  bodytext()
  p 
    margin-top 20px
  #page-introduction
    bodytext(27px,38px,bold)
    margin 50px 0px 30px
  img
    border-radius 100px
    -webkit-filter: grayscale(100%); /* Safari 6.0 - 9.0 */
    filter: grayscale(100%);
    width 200px
    height 200px
    padding 10px
    box-sizing border-box
    margin-top 20px
  h1
      margin 50px auto 25px    
  h2
    margin 40px 0px 20px
    
.form10 
  .wdform-element-section
  .wdform-label-section
  .wdform-field
  .wdform_column
  .wdform_section
    width 100%!important
  .wdform_footer    
    display none
  .wdform_page
    padding 0px!important
    background-color transparent!important
  .wdform-label
    tagtext(,,,uppercase)
  .wdform_row
    margin-top 20px
    &:first-of-type
      margin-top 0px
  .wdform-ch-rad-label
    bodytext()
  .wdform-element-section
    input[type=text]
      bodytext(19px)!important
      border-radius 5px
      padding 3px 10px!important
      width 100%important
      height 40px
      background-color white
      border 0
  margin 30px 0px 10px
  .button-submit
    font-family HK Grotesk!important
    justify-content center
    align-items center 
    background-color blue!important
    height 55px
    width 150px
    color white
    border-radius 5px        
    margin 0px!important
  .wdform-field
    padding 0px!important
    display block!important
  .fm-not-filled
    font-family HK Grotesk  
    color white
  .wdform-required
    display none
  .wdform-page-and-images
    padding 0px!important
    input[type=checkbox]
      width 25px
      height 25px
      appearance none
      -moz-appearance none
      -webkit-appearance none
      -o-appearance none
      -ms-appearance none
      background-color white
      border 0px solid blue
      position relative
      border-radius 2px
      &:after
        position absolute
        top 5px
        left 5px
        right 5px
        bottom 5px
        content ''      
        background-image: none;      
        background-size cover 
    input[type=checkbox]:checked 
      &:after
        position absolute
        top 5px
        left 5px
        right 5px
        bottom 5px
        content ''
        background-image: embedurl("../../../wp-content/themes/wpex-starter-theme-master/images/icon-cross-black.svg", "utf8");    
        background-repeat no-repeat
        background-size cover
        background-position center center       

#brand-application-section
  background-color green
  padding 30px
  margin 10px auto 40px
  h2
    margin-top 0px
    
#faq-section
  width 100%
  background-color blue10
  padding 50px
  .page-content
    background-color transparent   
   
@media only screen and (max-width:breakpoint3)
  #faq-section
    padding 0px 0px 40px

.brand-map-title
  width 100%
  margin-top 40px
  
.brand-map
  width 100%
  height 70vh
  overflow hidden
  background-color blue
  pointer-events none
@media only screen and (max-width:breakpoint3)
  .brand-map
    height 50vh
    
.shops-map
  width 100%
  height 600px
  margin 0 auto
  z-index 1
  position relative
@media only screen and (max-width:breakpoint3)
  .shops-map
    width 100%

.no-filter-results
  text-align center
  width 100%
  height 500px
  padding 20px
  margin-top 50px
  box-sizing border-box
  bodytext(21px,28px,bold)
@media only screen and (max-width:breakpoint4)
  margin-top 0px
  
#mobile-menu-slideout-panel
  display none
  height 100%
  width 0
  position fixed
  z-index 1000
  top 0
  left 0
  overflow-x hidden
  z-index 200
  transition .5s
  background-color blue
  text-align left
  box-sizing border-box
  margin-left 0
  bodytext(20px)
  ul
    li
      border-bottom 1px solid rgba(255,255,255,0.3)
      padding 25px 20px
      box-sizing border-box
      height 80px
  a
    color white
    text-decoration none    
    width 200px
    display block
    transition 0.3s

@media only screen and (max-width:breakpoint4)
  #mobile-menu-slideout-panel
    display block
    
.website-wrapper
  overflow-x hidden
  width 100%
  transition all 0.5s  

@media only screen and (max-width:breakpoint4)
  .website-wrapper-active
      margin-left 70vw
      width 100%
      overflow hidden

.mobile-menu-slideout-panel-active
  width 70vw!important
  transition all .5s

.footer-content
  display flex
  align-items flex-start
  max-width 1180px
  margin 0 auto 
  
footer
  background-color lightgrey
  color black
  width 100%
  height 100%
  padding 50px 50px 75px
  bodytext(16px,26px)
  h4
    margin-bottom 15px
    margin-left 10px
    position relative
    &:after
      position absolute
      content ''
      background-image: embedurl("../../../wp-content/themes/wpex-starter-theme-master/images/swash-06.svg", "utf8");        
      background-position left
      background-size contain
      top -10px
      left -10px
      background-repeat no-repeat
      mix-blend-mode multiply
      cursor pointer
      height 35px
      width 90px
  a
    color black!important
  .footer-content-section
    margin-right 50px
    flex 1   
    &:last-of-type
      margin-right 0px
    &:first-of-type
      flex 1
      //margin-top 30px
    img
      width 150px
      margin-top 20px
  span
    font-weight bold
    
@media only screen and (max-width:breakpoint3)
  .footer-content
    flex-direction column
  .footer-content-section
    &:first-of-type
      margin 15px auto
  footer
    padding 40px 20px
    div
      padding 0px
      &:first-of-type
        margin auto
        img
          width 200px
          

.wpgmza_table_marker
  display none 

.sorting, .dataTables_length, .dataTables_filter
  display none
  
.wpgmza_table_title  
  font-weight bold
  