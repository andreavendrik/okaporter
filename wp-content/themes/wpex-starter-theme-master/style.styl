    display block
    
.menu-items
  display flex
  align-items center
  justify-content center
  
.menu-item
  margin 5px 5px 5px 40px
  position relative
  ul
    cursor pointer
  &:hover
    &:after
      position absolute
      content ''
      background-image: embedurl("../../../wp-content/themes/wpex-starter-theme-master/images/swash-06.svg", "utf8");        
      background-position left
      background-size cover
      top 7px
      pointer-events none
      left calc(50% - 35px)
      background-repeat no-repeat
      mix-blend-mode multiply
      height 20px
      width 70px
  
@media only screen and (max-width:breakpoint4)
