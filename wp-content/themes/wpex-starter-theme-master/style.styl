.filter-button-active
  background-color grey
   
#menu-filter-button
  display none
  width 30px
  height 25px
  position relative
  overflow hidden
  svg 
    position absolute
    top 0px
    opacity 1
    transition all 0.5s
  span
    position absolute
    border-radius 3px
    height 2px
    background-color black
    transition all 0.5s
  #filter-line-1
    top 6px
    left 0px!important
    width 19px
    transform rotate(-135deg)
  #filter-line-2
    top 6px
    left 13px
    width 19px
    transform rotate(135deg)
  #filter-line-3
    top 11px
    width 4px!important
    left 14px
    height 40%!important
  #filter-line-4
    top 0px
    width 27px
    left 2px
    border-radius 0px!important
    display none

@media only screen and (max-width:breakpoint4)
  .home
    #menu-filter-button
      display flex
          
.menu-filter-button-active
  svg
    top -25px!important
    opacity 0
    fill blue
  span
    background-color blue!important
  #filter-line-1
    transform rotate(-45deg)!important
    top 10px!important
  #filter-line-2
    transform rotate(45deg)!important    
    top 10px!important
  #filter-line-3
    top -25px!important
  #filter-line-4
    top -3px!important

