@font-face
  font-family 'HK Grotesk'
  src: url('fonts/hkgrotesk-regular-webfont.woff') format('woff'),
       url('fonts/HKGrotesk-Regular.otf') format('truetype');
  font-style normal
  font-weight normal
  text-rendering optimizeLegibility

@font-face
  font-family 'HK Grotesk'
  src: url('fonts/hkgrotesk-medium-webfont.woff') format('woff'),
       url('fonts/HKGrotesk-Medium.otf') format('truetype');
  font-style bold
  font-weight 500
  text-rendering optimizeLegibility
  
@font-face
  font-family 'HK Grotesk'
  src: url('fonts/hkgrotesk-bold-webfont.woff') format('woff'),
       url('fonts/HKGrotesk-Bold.otf') format('truetype');
  font-style bold
  font-weight bold
  text-rendering optimizeLegibility

titletext(font-size=50px,line-height=45px)
  font-family 'HK Grotesk'
  font-size font-size
  line-height line-height
  font-weight bold

bodytext(font-size=18px,line-height=28px,font-weight=normal)
  font-family 'HK Grotesk'
  font-size font-size
  line-height line-height
  font-weight font-weight

tagtext(font-size=12px,font-weight=bold,line-height=14px,text-transform=none)
  font-family 'HK Grotesk'
  text-transform text-transform
  font-size font-size
  line-height line-height
  letter-spacing 0.5px
  font-weight font-weight

*
  box-sizing border-box
  padding 0
  margin 0

body
