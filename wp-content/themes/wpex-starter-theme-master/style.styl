.filter-dropdown
  box-sizing border-box
  tagtext(,bold,,capitalize) 
  ul
    display flex
    align-items center
    flex-wrap wrap
  input
    tagtext(,bold,,capitalize)  
    height 45px
    border 0px
    background-color white
    color blue
    text-align center     
    cursor pointer 
  input[type=checkbox]
    width 20px
    height 20px
    margin-right 20px
    appearance none
    -moz-appearance none
    -webkit-appearance none
    -o-appearance none
    -ms-appearance none
    background-color white
    border 0px solid blue
    position relative
    border-radius 2px
    &:after
      position absolute
      top 3px
      left 3px
      right 3px
      bottom 3px
      content ''      
      background-image: none;      
      background-size cover     
  input[type=submit]
    justify-content center
    align-items center 
    background-color blue
    height 55px
    width 150px
    color white
    border-radius 5px        
    margin-top 15px
  input[type=checkbox]:checked 
    &:after
      position absolute
      top 3px
      left 3px
      right 3px
      bottom 3px
      content ''
      background-image: embedurl("../../../wp-content/themes/wpex-starter-theme-master/images/icon-cross-black.svg", "utf8");    
      background-repeat no-repeat
      background-size cover
      background-position center center         
  label
